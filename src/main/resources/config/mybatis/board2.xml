<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="board1">

	<insert id="hotelAdd">
		insert into hotel
		values(hotel_seq.nextval,#{h_name},#{h_adress},#{h_image},0,#{h_content})
	</insert>
	<insert id="roomAdd">
		insert into room
		values(room_seq.nextval,#{h_name},#{r_name},#{r_price},#{r_service},#{r_inwon},#{r_cheap})
	</insert>

</mapper>